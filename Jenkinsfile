pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo '1111111Clarusway_Way to Reinvent Yourself'
                sh 'echo Integrating Jenkins Pipeline with GitHub Webhook using Jenkinsfile'
            }
        }

        stage('test') {
            steps {
                echo '222222222Clarusway_Way to Reinvent Yourself'
                sh 'whoami'
                sh 'pwd'
                sh 'ls'
            }
        }

        stage('run') {
            steps {
                echo '33333333333Clarusway_Way to Reinvent Yourself'
                sh 'python3 --version'
                sh 'python3 pipeline.py'
            }
        }

    }
}